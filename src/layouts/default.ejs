<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title><%= title || siteData.name %></title>
    <meta name="description" content="Come learn about Kippy!">

    <link rel="apple-touch-icon" sizes="180x180" href="/assets/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/assets/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/assets/favicon-16x16.png">
    <link rel="manifest" href="/assets/site.webmanifest">

    <link rel="stylesheet" href="/main.css"/>
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Mono&family=IBM+Plex+Sans&family=IBM+Plex+Serif:wght@600&display=fallback" rel="stylesheet">
  </head>
  <body
    class="transition-all"
    v-component="page"
    v-class:dark="darkMode"
  >
    <main class="k-container relative">
      <%- include('/page/navigation') %>
      <%- content %>
    </main>
    <%- include('/page/footer') %>
  </body>
  <script type="module" src="/main.js"></script>
</html>